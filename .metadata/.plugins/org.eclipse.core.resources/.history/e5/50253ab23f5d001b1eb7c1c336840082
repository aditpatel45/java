package com.assignment3;

public class BankApplication extends Customer {
	public void addCustomer(String name,String address,int accnum,double balance) {
		setAccnum(accnum);
		setAddress(address);
		setBalance(balance);
		setName(name);
		setId();
	}
	
	public void deleteCustomer(Customer cus) {
		cus=null;
	}
	
//	public void updateCustomer(Customer cus,int value) {
